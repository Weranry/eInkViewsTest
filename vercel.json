{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "regions": ["hkg1"],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}