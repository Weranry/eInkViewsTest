{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "^/favicon\\.ico$",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}
